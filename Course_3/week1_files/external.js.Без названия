x.to()),x.from())}e.scrollIntoView()})};h.sortLines=function(e){z(e,!0)};h.sortLinesInsensitive=function(e){z(e,!1)};h.nextBookmark=function(e){var f=e.state.sublimeBookmarks;if(f)for(;f.length;){var n=f.shift(),x=n.find();if(x)return f.push(n),e.setSelection(x.from,x.to)}};h.prevBookmark=function(e){var f=e.state.sublimeBookmarks;if(f)for(;f.length;){f.unshift(f.pop());var n=f[f.length-1].find();if(n)return e.setSelection(n.from,n.to);f.pop()}};h.toggleBookmark=function(e){for(var f=e.listSelections(),
n=e.state.sublimeBookmarks||(e.state.sublimeBookmarks=[]),x=0;x<f.length;x++){for(var G=f[x].from(),J=f[x].to(),E=f[x].empty()?e.findMarksAt(G):e.findMarks(G,J),T=0;T<E.length;T++)if(E[T].sublimeBookmark){E[T].clear();for(var P=0;P<n.length;P++)n[P]==E[T]&&n.splice(P--,1);break}T==E.length&&n.push(e.markText(G,J,{sublimeBookmark:!0,clearWhenEmpty:!1}))}};h.clearBookmarks=function(e){if(e=e.state.sublimeBookmarks)for(var f=0;f<e.length;f++)e[f].clear();e.length=0};h.selectBookmarks=function(e){var f=
e.state.sublimeBookmarks,n=[];if(f)for(var x=0;x<f.length;x++){var G=f[x].find();G?n.push({anchor:G.from,head:G.to}):f.splice(x--,0)}n.length&&e.setSelections(n,0)};h.smartBackspace=function(e){if(e.somethingSelected())return m.Pass;e.operation(function(){for(var f=e.listSelections(),n=e.getOption("indentUnit"),x=f.length-1;0<=x;x--){var G=f[x].head,J=e.getRange({line:G.line,ch:0},G),E=m.countColumn(J,null,e.getOption("tabSize")),T=e.findPosH(G,-1,"char",!1);J&&!/\S/.test(J)&&0==E%n&&(J=new l(G.line,
